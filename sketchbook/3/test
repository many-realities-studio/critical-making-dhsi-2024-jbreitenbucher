(function() {

    function setupP5(p) {
        let angle = 0;
        let angleSpeed = 0.05;

        p.setup = funtion() {
            let canvas = p.createCanvas(400, 400);
            canvas.parent('p5-canvas');
            p.angleMode(RADIANS);
        }

        p.draw = function() {
            p.background(30);
            p.drawAxe();
        }

        function drawAxe() {
            // Center point for swinging
            let pivotX = width / 2;
            let pivotY = height / 4;

            // Calculate axe position based on angle
            let axeLength = 200;
            let axeX = pivotX + axeLength * sin(angle);
            let axeY = pivotY + axeLength * cos(angle);

            // Update angle for swinging motion
            angle += angleSpeed;

            // Draw axe handle
            p.stroke(139, 69, 19); // brown color for handle
           p. strokeWeight(10);
            p.line(pivotX, pivotY, axeX, axeY);

            // Draw axe blade
            p.noStroke();
            p.fill(150); // gray color for blade
            p.push();
           p. translate(axeX, axeY);
           p. rotate(angle + PI / 2);
           p. rect(-20, 0, 40, 60);
            p.pop();
        }

        return { setup, draw };
    }

    // Wait for both libraries to load before running the setupP5 function
new p5(setupP5);

})();
//(live: 5s)[(goto: "Died")]